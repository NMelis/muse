{{ define "pages/index.tmpl" }}
<!DOCTYPE html>
<html lang="en">
  <head>
    {{ template "partials/meta" }}
    {{ template "partials/styles" }}
  </head>

  <body>
    <main>
      {{ range $song := .Collection }}
        <p>{{ $song.Artist }} / {{ $song.Album }} / {{ $song.Title }}</p>
      {{ end }}
    </main>
  </body>
</html>
{{ end }}
